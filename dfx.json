{
  "canisters": {
    "dtoken": {
      "main": "src/dtoken/src/registry.mo",
      "type": "motoko"
    },
	"dswap": {
	  "main": "src/dswap/src/DSwap.mo",
	  "type": "motoko"
	},
    "frontend": {
      "dependencies": [
        "dtoken", "dswap"
      ],
      "frontend": {
        "entrypoint": "src/frontend/index.jsx"
      },
      "source": [
        "src/frontend/",
        "dist/frontend/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
